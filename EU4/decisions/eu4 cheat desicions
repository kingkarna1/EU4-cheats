amount = 1000 }
#religion
every_owned_province = { limit = { NOT = { religion = FROM } }
change_religion = ROOT }
every_province = { limit = { AND = { owner = { is_subject_of = ROOT }
NOT = { religion = FROM } } }
change_religion = ROOT }
#core
every_owned_province = { limit = { NOT = { is_core = FROM } } 
add_core = ROOT }
every_province = { limit = { AND = { owner = { is_subject_of = ROOT }
NOT = { is_core = FROM } } }
add_core = ROOT
add_core = THIS }
#culture
every_owned_province = { limit = { NOT = { culture = FROM } }
change_culture = ROOT }
every_province = { limit = { AND = { owner = { is_subject_of = ROOT }
NOT = { culture = FROM } } }
change_culture = ROOT }
#dev
every_owned_province = { limit = { NOT = { base_manpower = 100 } }
set_base_manpower = 100 }
