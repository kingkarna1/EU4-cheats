add_building = counting_house }
every_owned_province = { limit = { NOT = { has_building = cathedral } }
add_building = cathedral }
#set to parlement
every_owned_province = { limit = { AND = { is_state = yes } }
set_seat_in_parliament = yes }
#build naval stuff
